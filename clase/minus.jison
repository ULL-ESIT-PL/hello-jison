%left '-' '+'
%left '*' '/'
%%
s: le EOF { return $1; }
;

le: le ',' e  { $$ = $1; $$.push($3); }
  | e         { $$ = [ $1 ]; }
;

e: e '-' e { $$ = $1 - $3; }
 | e '+' e { $$ = $1 + $3; }
 | e '*' e { $$ = $1 * $3; }
 | e '/' e { $$ = $1 / $3; }
 | '(' e ')' { $$ = $2; }
 | N         { $$ = Number($1); }
;
